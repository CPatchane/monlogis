h4 Ajouter un document
//- .insights
ol
  li Choisissez un fichier sur votre terminal :&ensp;
    input(type='file')

  li Donnez un nom clair à ce fichier :&ensp;
    input.form-control(name="filename", type="text", placeholder="notice.pdf")

  li Vous n'avez plus qu'à l'ajouter à votre Cozy :&ensp;
    button.btn.btn-primary(name='addfile', type="button") ajouter
